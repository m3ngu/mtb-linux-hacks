-#ifdef CONFIG_M386
-no_xadd: /* Legacy 386 processor */
-	local_irq_disable();
-	__i = atomic_read(v);
-	atomic_set(v, i + __i);
-	local_irq_enable();
-	return i + __i;
-#endif
-}
